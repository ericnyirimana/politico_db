    "bcrypt": "^3.0.4",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "dotenv": "^6.2.0",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.4.0",
    "moment": "^2.29.2",
    "morgan": "~1.9.0",
    "pg": "^7.8.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
